{
   "plugins": [
        "plugins/markdown",
        "plugins/summarize"
    ],
    "recurseDepth": 10,
    "source": {
        "include": [ "modules/core.mjs",
                     "modules/io.mjs",
                     "modules/tree.mjs",
                     "modules/webwindow.mjs",
                     "modules/painter.mjs",
                     "modules/draw.mjs",
                     "modules/latex.mjs",
                     "modules/gpad.mjs",
                     "modules/hist.mjs",
                     "modules/hist3d.mjs",
                     "modules/draw/more.mjs",
                     "modules/math.mjs",
                     "modules/base3d.mjs",
                     "modules/csg.mjs",
                     "modules/geobase.mjs",
                     "modules/geom.mjs",
                     "modules/display.mjs",
                     "modules/hierarchy.mjs",
                     "modules/menu.mjs",
                     "modules/v7gpad.mjs",
                     "modules/v7hist.mjs",
                     "modules/v7hist3d.mjs",
                     "modules/draw/v7more.mjs" ],
        "includePattern": ".+\\.mjs(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "opts": {
       "destination": "./jsdoc"
    },
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true,
        "name": "JSROOT",
        "default": {
           "outputSourceFiles" : true
        },
        "applicationName": "JavaScript ROOT",
        "disqus": "",
        "googleAnalytics": "",
        "openGraph": {
          "title": "",
          "type": "website",
          "image": "",
          "site_name": "",
          "url": ""
         },
         "meta": {
            "title": "",
            "description": "",
            "keyword": ""
          },
         "linenums": false
    }
}
